{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "redirects": [
        {
            "source": "/",
            "destination": "/prayertimes",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/prayertimes",
            "destination": "/"
        },
        {
            "source": "/prayertimes/(.*)",
            "destination": "/"
        }
    ]
}