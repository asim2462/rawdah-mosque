{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "redirects": [
        {
            "source": "/",
            "destination": "/prayertimes",
            "permanent": true
        },
        {
            "source": "/:path((?!prayertimes).*)",
            "destination": "/prayertimes",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/prayertimes",
            "destination": "/index.html"
        },
        {
            "source": "/prayertimes/(.*)",
            "destination": "/index.html"
        }
    ]
}